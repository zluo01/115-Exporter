{
  "name": "115-exporter",
  "displayName": "115 File Exporter",
  "description": "Helper to download files from 115 through Aria2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build && web-ext run --source-dir ./build/",
    "build": "npm run type-check && vite build",
    "package": "web-ext build --config=webext.config.js",
    "complete": "npm run build && npm run package",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js"
  },
  "dependencies": {
    "@headlessui/react": "1.7.17",
    "aria2": "4.1.2",
    "big-integer": "1.6.52",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.10.6",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@types/webextension-polyfill": "0.10.7",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-standard": "5.0.0",
    "postcss": "8.4.33",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.11",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.0.11",
    "vite-plugin-css-injected-by-js": "3.3.1",
    "web-ext": "7.9.0",
    "webextension-polyfill": "0.10.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
